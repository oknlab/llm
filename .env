NGROK_AUTH_TOKEN='1vikehg18jsR9XrEzKEybCifEr9_AWWFzoCD58Xa151mXfLd'
HOST=0.0.0.0
PORT=8000
ENV=production
LOG_LEVEL=INFO
DEBUG=false

# ═══════════════════════════════════════════════════════════
# MODEL CONFIGURATION
# ═══════════════════════════════════════════════════════════
MODEL_NAME=Qwen/Qwen3-1.7B
DEVICE=cuda
MAX_LENGTH=2048
TEMPERATURE=0.7

# ═══════════════════════════════════════════════════════════
# RAG CONFIGURATION
# ═══════════════════════════════════════════════════════════
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
CHUNK_SIZE=512
CHUNK_OVERLAP=50
TOP_K=5
RERANK_ENABLED=true

# ═══════════════════════════════════════════════════════════
# SCRAPING
# ═══════════════════════════════════════════════════════════
CSE_URL=https://cse.google.com/cse?cx=014662525286492529401:2upbuo2qpni
MAX_SCRAPE_PAGES=10
REQUEST_TIMEOUT=30

# ═══════════════════════════════════════════════════════════
# AIRFLOW
# ═══════════════════════════════════════════════════════════
AIRFLOW_HOME=./airflow
AIRFLOW_WEBSERVER_PORT=8080
AIRFLOW_DB_URI=postgresql+psycopg2://airflow:airflow@localhost:5432/airflow
AIRFLOW_EXECUTOR=LocalExecutor

# ═══════════════════════════════════════════════════════════
# INTEGRATIONS - SLACK
# ═══════════════════════════════════════════════════════════
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_APP_TOKEN=xapp-your-slack-app-token
SLACK_CHANNEL=#ai-agents

# ═══════════════════════════════════════════════════════════
# INTEGRATIONS - NOTION
# ═══════════════════════════════════════════════════════════
NOTION_API_KEY=secret_your_notion_key
NOTION_DATABASE_ID=your_database_id

# ═══════════════════════════════════════════════════════════
# INTEGRATIONS - GITHUB
# ═══════════════════════════════════════════════════════════
GITHUB_TOKEN=ghp_your_github_token
GITHUB_ORG=your-org
GITHUB_REPO=your-repo

# ═══════════════════════════════════════════════════════════
# INTEGRATIONS - CONFLUENCE
# ═══════════════════════════════════════════════════════════
CONFLUENCE_URL=https://your-domain.atlassian.net
CONFLUENCE_USERNAME=your-email@domain.com
CONFLUENCE_API_TOKEN=your_confluence_token
CONFLUENCE_SPACE=YOURSPACE

# ═══════════════════════════════════════════════════════════
# INTEGRATIONS - ZAPIER / N8N / MAKE
# ═══════════════════════════════════════════════════════════
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/xxx/xxx/
N8N_WEBHOOK_URL=https://your-n8n.app/webhook/xxx
MAKE_WEBHOOK_URL=https://hook.integromat.com/xxx

# ═══════════════════════════════════════════════════════════
# DATA PIPELINES - KAFKA
# ═══════════════════════════════════════════════════════════
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC_AGENTS=agent-events
KAFKA_TOPIC_RAG=rag-queries

# ═══════════════════════════════════════════════════════════
# DATA PIPELINES - S3 / MINIO
# ═══════════════════════════════════════════════════════════
S3_ENDPOINT=https://s3.amazonaws.com
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key
S3_BUCKET=ai-agent-data
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin

# ═══════════════════════════════════════════════════════════
# MONITORING - PROMETHEUS / GRAFANA
# ═══════════════════════════════════════════════════════════
PROMETHEUS_PORT=9090
GRAFANA_URL=http://localhost:3000
GRAFANA_API_KEY=your_grafana_api_key

# ═══════════════════════════════════════════════════════════
# MODEL MANAGEMENT - MLFLOW / W&B
# ═══════════════════════════════════════════════════════════
MLFLOW_TRACKING_URI=http://localhost:5000
WANDB_API_KEY=your_wandb_api_key
WANDB_PROJECT=ai-agent-system
WANDB_ENTITY=your-team

# ═══════════════════════════════════════════════════════════
# DATABASE & CACHING
# ═══════════════════════════════════════════════════════════
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=agents
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# ═══════════════════════════════════════════════════════════
# SELF-IMPROVEMENT
# ═══════════════════════════════════════════════════════════
ENABLE_SELF_IMPROVEMENT=true
FEEDBACK_THRESHOLD=0.7
LEARNING_RATE=0.001
IMPROVEMENT_INTERVAL=3600
